{
  "manifest_version": 2,
  "name": "Lemmy.ml Karma Calculator",
  "version": "1.1",
  "description": "Display Lemmy.ml Karma.",
  "permissions": ["activeTab", "storage"],
  "browser_action": {
    "default_icon": {
      "128": "thumbnail.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"]
    }
  ]
}
